{
  "name": "@nice-digital/cks-gatsby",
  "private": true,
  "version": "1.0.0",
  "description": "Website for NICE Clinical Knowledge Summaries (CKS) built with Gatsby and using the NICE Design System",
  "scripts": {
    "start": "npm run develop",
    "// GATSBY": "",
    "develop": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "cb": "gatsby info --clipboard",
    "clean": "gatsby clean",
    "gatsby": "gatsby",
    "// UTILS": "",
    "wait": "wait-on tcp:localhost:8000",
    "// TESTS": "",
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --watch",
    "test:coverage": "npm run test -- --coverage",
    "// LINTING/CODE STYLE": "",
    "lint": "npm run prettier && npm run lint:ts && npm run typecheck",
    "prettier": "prettier --check \"./**/*.{ts,tsx,js}\"",
    "prettier:fix": "npm run prettier -- --write",
    "lint:ts": "eslint \"**/*.{js,jsx,ts,tsx}\"",
    "lint:ts:fix": "npm run lint:ts -- --fix",
    "// TYPECHECKING": "",
    "typecheck": "tsc --noEmit",
    "//Teamcity": "",
    "audit:teamcity": "npm audit --audit-level=critical"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nice-digital/cks-gatsby.git"
  },
  "keywords": [
    "gatsby",
    "gatsbyjs"
  ],
  "engines": {
    "node": ">=18",
    "npm": ">=9"
  },
  "author": "NICE Digital (https://www.nice.org.uk/digital)",
  "contributors": [
    "NICE Digital Services (https://www.nice.org.uk/digital)",
    "Ian Routledge (http://ediblecode.com)"
  ],
  "license": "MIT",
  "homepage": "https://github.com/nice-digital/cks-gatsby#readme",
  "browserslist": [
    "extends @nice-digital/browserslist-config"
  ],
  "devDependencies": {
    "@nice-digital/browserslist-config": "^2.0.1",
    "@nice-digital/design-system": "^5.0.15",
    "@nice-digital/global-nav": "^7.1.1048",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/async": "^3.2.15",
    "@types/jest": "^29.5.8",
    "@types/lodash.throttle": "^4.1.7",
    "@types/node": "^20.9.0",
    "@types/node-fetch": "^2.6.2",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-helmet": "^6.1.5",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "async": "^3.2.4",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-testing-library": "^6.1.2",
    "gatsby": "^5.12.9",
    "gatsby-plugin-catch-links": "^5.12.0",
    "gatsby-plugin-eslint": "^4.0.4",
    "gatsby-plugin-google-tagmanager": "^5.12.3",
    "gatsby-plugin-manifest": "^5.12.3",
    "gatsby-plugin-offline": "^6.12.3",
    "gatsby-plugin-react-helmet": "^6.12.0",
    "gatsby-plugin-sass": "^6.12.3",
    "gatsby-plugin-sitemap": "^6.12.3",
    "gatsby-plugin-split-css": "^2.0.3",
    "gatsby-plugin-typescript": "^5.12.1",
    "http-proxy-middleware": "^2.0.6",
    "identity-obj-proxy": "^3.0.0",
    "install": "^0.13.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-teamcity-reporter": "^0.9.0",
    "lodash.throttle": "^4.1.1",
    "module": "^1.2.5",
    "node-fetch": "^2.6.7",
    "node-notifier": "^10.0.1",
    "prettier": "^3.1.0",
    "prettier-2": "npm:prettier@^2",
    "process": "^0.11.10",
    "react": "^18.0.2",
    "react-dom": "^18.0.2",
    "react-helmet": "^6.1.0",
    "sass": "^1.54.0",
    "slugify": "^1.6.5",
    "source-map-support": "^0.5.20",
    "stream-browserify": "^3.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "wait-on": "^7.1.0",
    "web-vitals": "^2.1.4"
  },
  "volta": {
    "node": "18.18.2"
  }
}
