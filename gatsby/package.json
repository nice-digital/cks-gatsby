{
  "name": "@nice-digital/cks-gatsby",
  "private": true,
  "version": "1.0.0",
  "description": "Website for NICE Clinical Knowledge Summaries (CKS) built with Gatsby and using the NICE Design System",
  "scripts": {
    "start": "npm run develop",
    "// GATSBY": "",
    "develop": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "cb": "gatsby info --clipboard",
    "clean": "gatsby clean",
    "gatsby": "gatsby",
    "// UTILS": "",
    "wait": "wait-on tcp:localhost:8000",
    "// TESTS": "",
    "test": "jest --notify --detectOpenHandles",
    "test:watch": "jest --watch",
    "test:coverage": "npm run test -- --coverage",
    "// LINTING/CODE STYLE": "",
    "lint": "npm run prettier && npm run lint:ts && npm run typecheck",
    "prettier": "prettier --check \"./**/*.{ts,tsx,js}\"",
    "prettier:fix": "npm run prettier -- --write",
    "lint:ts": "eslint \"**/*.{js,jsx,ts,tsx}\"",
    "lint:ts:fix": "npm run lint:ts -- --fix",
    "// TYPECHECKING": "",
    "typecheck": "tsc --noEmit",
    "//Teamcity": "",
    "audit:teamcity": "npm audit --audit-level=moderate --json|npm-audit-reporter-teamcity"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nice-digital/cks-gatsby.git"
  },
  "keywords": [
    "gatsby",
    "gatsbyjs"
  ],
  "engines": {
    "node": ">=10",
    "npm": ">=6"
  },
  "author": "NICE Digital (https://www.nice.org.uk/digital)",
  "contributors": [
    "NICE Digital Services (https://www.nice.org.uk/digital)",
    "Ian Routledge (http://ediblecode.com)"
  ],
  "license": "MIT",
  "homepage": "https://github.com/nice-digital/cks-gatsby#readme",
  "browserslist": [
    "extends @nice-digital/browserslist-config"
  ],
  "devDependencies": {
    "@nice-digital/browserslist-config": "^2.0.1",
    "@nice-digital/design-system": "^3.0.2-alpha.0",
    "@nice-digital/global-nav": "^5.0.865-GN-187-Rebrand",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^13.2.1",
    "@types/async": "^3.2.15",
    "@types/jest": "^26.0.24",
    "@types/lodash.throttle": "^4.1.7",
    "@types/node": "^14.18.22",
    "@types/node-fetch": "^2.6.2",
    "@types/react": "^17.0.47",
    "@types/react-dom": "^17.0.17",
    "@types/react-helmet": "^6.1.5",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "async": "^3.2.4",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-testing-library": "^4.12.4",
    "gatsby": "^3.14.2",
    "gatsby-plugin-catch-links": "^3.14.0",
    "gatsby-plugin-eslint": "^3.0.0",
    "gatsby-plugin-google-tagmanager": "^3.14.0",
    "gatsby-plugin-manifest": "^3.14.0",
    "gatsby-plugin-offline": "^4.14.0",
    "gatsby-plugin-preact": "^5.14.0",
    "gatsby-plugin-react-helmet": "^4.14.0",
    "gatsby-plugin-remove-generator": "^1.2.0",
    "gatsby-plugin-sass": "^4.14.0",
    "gatsby-plugin-sitemap": "^4.10.0",
    "gatsby-plugin-split-css": "^2.0.2",
    "gatsby-plugin-typescript": "^3.14.0",
    "http-proxy-middleware": "^2.0.6",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-fetch-mock": "^3.0.3",
    "jest-teamcity-reporter": "^0.9.0",
    "lodash.throttle": "^4.1.1",
    "node-fetch": "^2.6.7",
    "node-notifier": "^10.0.1",
    "npm-audit-reporter-teamcity": "^0.3.1",
    "preact": "^10.10.0",
    "preact-render-to-string": "^5.2.1",
    "prettier": "^2.7.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "sass": "^1.53.0",
    "slugify": "^1.6.5",
    "source-map-support": "^0.5.20",
    "ts-jest": "^26.5.6",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "wait-on": "^6.0.1",
    "web-vitals": "^2.1.4"
  }
}
