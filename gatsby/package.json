    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "slugify": "^1.4.4",
    "snapshot-diff": "^0.8.1",
    "source-map-support": "^0.5.19",
    "ts-jest": "^26.1.3",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7",
    "wait-on": "^5.1.0"
  },
  "resolutions": {
    "gatsby-react-router-scroll": "3.0.0"
  },
  "dependencies": {}
