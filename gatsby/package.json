{
  "name": "@nice-digital/cks-gatsby",
  "private": true,
  "version": "1.0.0",
  "description": "Website for NICE Clinical Knowledge Summaries (CKS) built with Gatsby and using the NICE Design System",
  "scripts": {
    "start": "npm run develop",
    "// GATSBY": "",
    "develop": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "cb": "gatsby info --clipboard",
    "clean": "gatsby clean",
    "gatsby": "gatsby",
    "// UTILS": "",
    "wait": "wait-on tcp:localhost:8000",
    "// TESTS": "",
    "test": "jest --notify --silent",
    "test:watch": "jest --watch --silent",
    "test:coverage": "npm run test -- --coverage",
    "// LINTING/CODE STYLE": "",
    "lint": "npm run prettier && npm run lint:ts && npm run typecheck",
    "prettier": "prettier --check \"src/**/*.{ts,tsx,js}\"",
    "prettier:fix": "prettier --check \"src/**/*.{ts,tsx,js}\" --write",
    "lint:ts": "eslint \"**/*.{js,jsx,ts,tsx}\"",
    "lint:ts:fix": "npm run lint:ts -- --fix",
    "// TYPECHECKING": "",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nice-digital/cks-gatsby.git"
  },
  "keywords": [
    "gatsby",
    "gatsbyjs"
  ],
  "engines": {
    "node": ">=10",
    "npm": ">=6"
  },
  "author": "NICE Digital (https://www.nice.org.uk/digital)",
  "contributors": [
    "NICE Digital Services (https://www.nice.org.uk/digital)",
    "Ian Routledge (http://ediblecode.com)"
  ],
  "license": "MIT",
  "homepage": "https://github.com/nice-digital/cks-gatsby#readme",
  "devDependencies": {
    "@nice-digital/design-system": "^1.0.33-alpha.0",
    "@nice-digital/global-nav": "git://github.com/nice-digital/global-nav.git#44edfb9e63ad4a2290bfd7aa981cb9164fc60f4f",
    "@testing-library/jest-dom": "^5.11.0",
    "@testing-library/react": "^10.4.5",
    "@testing-library/user-event": "^12.0.11",
    "@types/async": "^3.2.3",
    "@types/jest": "^26.0.4",
    "@types/node": "^14.0.22",
    "@types/node-fetch": "^2.5.7",
    "@types/react": "^16.9.42",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "async": "^3.2.0",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-testing-library": "^3.3.1",
    "gatsby": "^2.24.2",
    "gatsby-plugin-eslint": "^2.0.8",
    "gatsby-plugin-google-tagmanager": "^2.3.11",
    "gatsby-plugin-manifest": "^2.4.18",
    "gatsby-plugin-offline": "^3.2.17",
    "gatsby-plugin-prefetch-google-fonts": "^1.4.3",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-plugin-sass": "^2.3.12",
    "gatsby-plugin-sitemap": "^2.4.11",
    "gatsby-plugin-typescript": "^2.4.14",
    "http-proxy-middleware": "^1.0.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.1.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-teamcity-reporter": "^0.9.0",
    "node-fetch": "^2.6.0",
    "node-sass": "^4.14.1",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "slugify": "^1.4.4",
    "snapshot-diff": "^0.8.1",
    "source-map-support": "^0.5.19",
    "ts-jest": "^26.1.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.6",
    "wait-on": "^5.0.3"
  }
}
