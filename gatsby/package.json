{
  "name": "@nice-digital/cks-gatsby",
  "private": true,
  "version": "1.0.0",
  "description": "Website for NICE Clinical Knowledge Summaries (CKS) built with Gatsby and using the NICE Design System",
  "scripts": {
    "preinstall": "npx npm-force-resolutions",
    "start": "npm run develop",
    "// GATSBY": "",
    "develop": "cross-env GATSBY_HOT_LOADER=fast-refresh gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "cb": "gatsby info --clipboard",
    "clean": "gatsby clean",
    "gatsby": "gatsby",
    "// UTILS": "",
    "wait": "wait-on tcp:localhost:8000",
    "// TESTS": "",
    "test": "jest --notify",
    "test:watch": "jest --watch",
    "test:coverage": "npm run test -- --coverage",
    "// LINTING/CODE STYLE": "",
    "lint": "npm run prettier && npm run lint:ts && npm run typecheck",
    "prettier": "prettier --check \"./**/*.{ts,tsx,js}\"",
    "prettier:fix": "npm run prettier -- --write",
    "lint:ts": "eslint \"**/*.{js,jsx,ts,tsx}\"",
    "lint:ts:fix": "npm run lint:ts -- --fix",
    "// TYPECHECKING": "",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nice-digital/cks-gatsby.git"
  },
  "keywords": [
    "gatsby",
    "gatsbyjs"
  ],
  "engines": {
    "node": ">=10",
    "npm": ">=6"
  },
  "author": "NICE Digital (https://www.nice.org.uk/digital)",
  "contributors": [
    "NICE Digital Services (https://www.nice.org.uk/digital)",
    "Ian Routledge (http://ediblecode.com)"
  ],
  "license": "MIT",
  "homepage": "https://github.com/nice-digital/cks-gatsby#readme",
  "devDependencies": {
    "@nice-digital/design-system": "^1.0.40-alpha.0",
    "@nice-digital/global-nav": "git://github.com/nice-digital/global-nav.git#1637e361cfd5de538b053b424c581f6f104dcd75",
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.1.1",
    "@testing-library/user-event": "^12.2.0",
    "@types/async": "^3.2.3",
    "@types/jest": "^26.0.15",
    "@types/lodash.throttle": "^4.1.6",
    "@types/node": "^14.14.6",
    "@types/node-fetch": "^2.5.7",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "@types/react-helmet": "^6.1.0",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "async": "^3.2.0",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-testing-library": "^3.10.0",
    "gatsby": "^2.25.3",
    "gatsby-plugin-catch-links": "^2.3.15",
    "gatsby-plugin-eslint": "^2.0.8",
    "gatsby-plugin-google-tagmanager": "^2.4.0",
    "gatsby-plugin-manifest": "2.4.21",
    "gatsby-plugin-offline": "^3.3.2",
    "gatsby-plugin-preact": "^4.0.16",
    "gatsby-plugin-prefetch-google-fonts": "^1.4.3",
    "gatsby-plugin-react-helmet": "^3.3.14",
    "gatsby-plugin-sass": "^2.4.2",
    "gatsby-plugin-sitemap": "^2.5.1",
    "gatsby-plugin-split-css": "^1.0.2",
    "gatsby-plugin-typescript": "^2.5.0",
    "gatsby-react-router-scroll": "3.0.0",
    "http-proxy-middleware": "^1.0.6",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-fetch-mock": "^3.0.3",
    "jest-teamcity-reporter": "^0.9.0",
    "lodash.throttle": "^4.1.1",
    "node-fetch": "^2.6.1",
    "preact": "^10.5.5",
    "preact-render-to-string": "^5.1.11",
    "prettier": "^2.1.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-helmet": "^6.1.0",
    "sass": "^1.29.0",
    "slugify": "^1.4.6",
    "snapshot-diff": "^0.8.1",
    "source-map-support": "^0.5.19",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5",
    "wait-on": "^5.2.0"
  },
  "resolutions": {
    "gatsby-react-router-scroll": "3.0.0"
  },
  "dependencies": {}
}
